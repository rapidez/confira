{
    "name": "rapidez/confira",
    "description": "Rapidez Confira Checkout Theme",
    "keywords": [
        "rapidez",
        "confira"
    ],
    "homepage": "https://rapidez.io",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Roy Duineveld",
            "email": "royduineveld@gmail.com",
            "homepage": "https://royduineveld.nl",
            "role": "Developer"
        },
        {
            "name": "Roene",
            "email": "roene@justbetter.nl",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "rapidez/checkout-theme": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Rapidez\\Confira\\": "src"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rapidez\\Confira\\ConfiraServiceProvider"
            ]
        }
    }
}
